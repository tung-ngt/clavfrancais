{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "clavfrancais",
  "version": "0.1.0",
  "identifier": "com.tungngt.clavfrancais",
  "build": {
    "beforeDevCommand": "npm run dev",
    "devUrl": "http://localhost:1420",
    "beforeBuildCommand": "npm run build",
    "frontendDist": "../dist"
  },
  "app": {
    "windows": [
      {
        "title": "clavfrancais",
        "minWidth": 500,
        "minHeight": 300,
        "width": 500,
        "height": 300,
        "visible": false,
        "contentProtected": true
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ],
    "resources": [
        "resources/*"
    ]
  }
}
